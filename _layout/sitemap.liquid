<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for dir in dirs %}
        {% for page in dir[1] %}
            <url>
                <loc>{{ site.url }}{{ page.permalink }}</loc>
                <lastmod>{{ page.updated }}</lastmod>
            </url>
        {% endfor %}
    {% endfor %}
</urlset>